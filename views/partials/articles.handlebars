<ul class="list-group">
    {{#each article}}
        <li class="list-group-item">
            <h4 class="list-group-item-heading">{{this.title}}</h4>
            <a target="_blank" href="{{this.link}}" class="list-group-item-text">{{this.link}}</a>
            <div class="form-group">
                <form action="/articles/{{this._id}}" method="get">

                        <button type="submit" class="btn btn-primary ">Save Article</button>

                </form>
            </div>
        </li>
    {{/each}}
</ul>